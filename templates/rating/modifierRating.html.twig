		              <h4 class="text-dark mb-3">Edit rating</h4>
                            {{ form_start(form) }}
                            {% do form.rating.setRendered() %}

                            {% set checked1 = "" %} {% set checked2 = "" %} {% set checked3 = "" %} {% set checked4 = "" %} {% set checked5 = "" %}
                            {% set ratingInt = rating.rating|number_format %}
                            {% if rating is not null %}
                                {% if rating.rating == 1 %}
                                    {% set checked1 = "checked" %} {% set checked2 = "" %} {% set checked3 = "" %} {% set checked4 = "" %} {% set checked5 = "" %}
                                {% elseif rating.rating == 2 %}
                                    {% set checked1 = "" %} {% set checked2 = "checked" %} {% set checked3 = "" %} {% set checked4 = "" %} {% set checked5 = "" %}
                                {% elseif rating.rating == 3 %}
                                    {% set checked1 = "" %} {% set checked2 = "" %} {% set checked3 = "checked" %} {% set checked4 = "" %} {% set checked5 = "" %}
                                {% elseif rating.rating == 4 %}
                                    {% set checked1 = "" %} {% set checked2 = "" %} {% set checked3 = "" %} {% set checked4 = "checked" %} {% set checked5 = "" %}
                                {% elseif rating.rating == 5 %}
                                    {% set checked1 = "" %} {% set checked2 = "" %} {% set checked3 = "" %} {% set checked4 = "" %} {% set checked5 = "checked" %}
                                {% endif %}
                            {% endif %}

                            <div class="row">
                                <!-- <label for="rating_rating">Note :</label> -->
                                <div id="rating_rating" class="rate">
                                    <input type="radio" id="rating_rating_0" name="rating[rating]"
                                           value="5" {{ checked5 }}/>
                                    <label for="rating_rating_0" title="text">1 stars</label>
                                    <input type="radio" id="rating_rating_1" name="rating[rating]"
                                           value="4" {{ checked4 }}/>
                                    <label for="rating_rating_1" title="text">2 stars</label>
                                    <input type="radio" id="rating_rating_2" name="rating[rating]"
                                           value="3" {{ checked3 }}/>
                                    <label for="rating_rating_2" title="text">3 stars</label>
                                    <input type="radio" id="rating_rating_3" name="rating[rating]"
                                           value="2" {{ checked2 }}/>
                                    <label for="rating_rating_3" title="text">4 stars</label>
                                    <input type="radio" id="rating_rating_4" name="rating[rating]"
                                           value="1" {{ checked1 }}/>
                                    <label for="rating_rating_4" title="text">5 star</label>
                                </div>
                            </div>
                            {{ form_errors(form.rating) }}
                            <div class="row">
                                <div class="col-md-12">
                                    <div class="form-group app-label mt-2">
                                        {{ form_label(form.nom) }}
