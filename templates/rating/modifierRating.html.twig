                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-lg-8 m-auto">
                                    {{ form_widget(form.submit, {
                                        'attr': {
                                            'class': 'form-control btn btn-primary',
                                        }
                                    }) }}
                                </div>
                            </div>
                            {{ form_end(form) }}
